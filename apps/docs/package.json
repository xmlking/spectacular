{
	"name": "docs",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev --port 3002",
		"dev:debug": "DEBUG=* vite dev",
		"dev:sw": "SW_DEV=true vite dev",
		"build": "vite build",
		"preview": "NEXTAUTH_URL=http://localhost:4173 vite preview",
		"test": "pnpm run test:integration && pnpm run test:unit",
		"test:unit": "vitest",
		"test:unit:ui": "vitest --ui",
		"test:unit:coverage": "vitest run --coverage",
		"test:integration": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --ignore-path=../../.prettierignore . && eslint .",
		"format": "prettier --write --ignore-path=../../.prettierignore .",
		"gh-pages": "pnpm run build && echo kashmora.com > ./build/CNAME && touch ./build/.nojekyll && pnpx gh-pages -d build -t true"
	},
	"devDependencies": {
		"@ethercorps/sveltekit-og": "2.0.2",
		"@floating-ui/dom": "1.5.3",
		"@indaco/svelte-iconoir": "6.0.3",
