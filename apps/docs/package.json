{
	"name": "docs",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --port 3002",
		"dev:debug": "DEBUG=* vite dev",
		"dev:sw": "SW_DEV=true vite dev",
		"build": "vite build",
		"preview": "NEXTAUTH_URL=http://localhost:4173 vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"test:unit": "vitest",
		"test:unit:ui": "vitest --ui",
		"test:unit:coverage": "vitest run --coverage",
		"test:integration": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check --ignore-path=../../.prettierignore . && eslint .",
		"format": "prettier --plugin-search-dir . --write --ignore-path=../../.prettierignore ."
	},
	"devDependencies": {
		"@playwright/test": "1.37.1",
		"@sveltejs/adapter-auto": "2.1.0",
		"@sveltejs/kit": "1.25.0",
		"@tailwindcss/container-queries": "0.1.1",
		"@vercel/analytics": "1.0.2",
		"eslint-config-custom": "workspace:*",
		"svelte": "4.2.0",
		"svelte-adapter-bun": "0.5.0",
		"svelte-check": "3.5.1",
		"svelte-meta-tags": "3.0.3",
		"sveltekit-rate-limiter": "0.4.1",
		"tailwind-merge": "1.14.0",
		"tailwindcss-animate": "1.0.7",
		"tslib": "2.6.2",
		"typescript": "5.2.2",
		"ui": "workspace:*",
		"vite": "4.4.9",
		"vitest": "0.34.4"
	},
	"optionalDependencies": {
		"@playwright/test": "1.38.0",
		"@vitest/ui": "0.34.4"
	},
	"type": "module"
}
