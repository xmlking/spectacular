		"autoprefixer": "10.4.16",
		"eslint": "8.53.0",
		"eslint-config-custom": "workspace:*",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-svelte": "2.35.0",
		"formsnap": "0.4.1",
		"lodash-es": "4.17.21",
		"mdsvex": "0.11.0",
