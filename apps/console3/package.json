		"@typescript-eslint/parser": "6.14.0",
		"eslint": "8.56.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.35.1",
		"prettier": "3.1.1",
		"prettier-plugin-svelte": "3.1.2",
		"svelte": "4.2.8",
		"svelte-check": "3.6.2",
		"tslib": "2.6.2",
		"typescript": "5.3.3",
		"vite": "5.0.10",
		"vitest": "1.0.4"
