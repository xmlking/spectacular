{
	"name": "console-sc",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "dotenv-run -f ../../.env -f ../../.secrets -f .env -f .secrets -v -- vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "dotenv-run -f ../../.env -f ../../.secrets -f .env -f .secrets -v -- vite dev",
		"dev:debug": "DEBUG=* dotenv-run -f ../../.env -f ../../.secrets -f .env -f .secrets -v '.*' -u -- vite dev",
		"dev:sw": "SW_DEV=true dotenv-run -f ../../.env -f ../../.secrets -f .env -f .secrets -v -- vite dev",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"preview": "ORIGIN=https://console.traefik.me:4173 dotenv-run -f ../../.env -f ../../.secrets -f .env -f .secrets -v -- vite preview",
		"sync": "dotenv-run -f ../../.env -f ../../.secrets -f .env -f .secrets -v -- svelte-kit sync",
		"test": "pnpm run test:integration && pnpm run test:unit",
		"test:integration": "dotenv-run -f ../../.env -f ../../.secrets -f .env -f .secrets -v -- playwright test",
		"test:unit": "dotenv-run -f ../../.env -f ../../.secrets -f .env -f .secrets -v -- vitest",
		"test:unit:coverage": "dotenv-run -f ../../.env -f ../../.secrets -f .env -f .secrets -v -- vitest run --coverage",
		"test:unit:ui": "dotenv-run -f ../../.env -f ../../.secrets -f .env -f .secrets -v -- vitest --ui"
	},
	"devDependencies": {
		"@fontsource-variable/inter": "5.0.17",
		"@internationalized/date": "3.5.2",
		"@spectacular/eslint-config": "workspace:*",
		"@spectacular/prettier-config": "workspace:*",
		"@spectacular/typescript-config": "workspace:*",
		"@spectacular/ui": "workspace:*",
		"@spectacular/utils": "workspace:*",
		"@sveltejs/adapter-auto": "3.1.1",
		"@sveltejs/enhanced-img": "0.1.9",
		"@sveltejs/kit": "2.5.4",
		"@sveltejs/vite-plugin-svelte": "3.0.2",
		"@tailwindcss/container-queries": "0.1.1",
		"@tailwindcss/forms": "0.5.7",
		"@tailwindcss/typography": "0.5.10",
		"@types/eslint": "8.56.6",
		"@types/node": "20.11.30",
		"@typescript-eslint/eslint-plugin": "7.3.1",
		"@typescript-eslint/parser": "7.3.1",
		"@vincjo/datatables": "1.14.5",
		"autoprefixer": "10.4.19",
		"bits-ui": "0.20.0",
		"clsx": "2.1.0",
		"cmdk-sv": "0.0.15",
		"cssnano": "6.1.1",
		"eslint": "8.57.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.36.0-next.6",
		"formsnap": "0.5.1",
		"lucide-svelte": "0.359.0",
		"postcss": "8.4.38",
		"postcss-import": "16.1.0",
		"postcss-load-config": "5.0.3",
		"postcss-nested": "6.0.1",
		"prettier": "3.2.5",
		"prettier-plugin-svelte": "3.2.2",
		"prettier-plugin-tailwindcss": "0.5.12",
		"svelte": "4.2.12",
		"svelte-check": "3.6.8",
		"svelte-radix": "1.1.0",
		"sveltekit-rate-limiter": "0.5.1",
		"sveltekit-superforms": "2.10.6",
		"sveltekit-view-transition": "0.5.3",
		"tailwind-merge": "2.2.2",
		"tailwind-variants": "0.2.1",
		"tailwindcss": "3.4.1",
		"tailwindcss-animate": "1.0.7",
		"tslib": "2.6.2",
		"typescript": "5.4.3",
		"vite": "5.2.2",
		"vite-plugin-tailwind-purgecss": "0.2.0",
		"vitest": "1.4.0",
		"zod": "3.22.4"
	},
	"optionalDependencies": {
		"@playwright/test": "1.42.1",
		"@vitest/ui": "1.4.0"
	}
}
