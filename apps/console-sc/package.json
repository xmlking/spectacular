{
	"name": "console-sc",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "dotenv-run -f .env -f .secrets -v -- vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "dotenv-run -f .env -f .secrets -v -- vite dev",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"preview": "dotenv-run -f .env -f .secrets -v -- vite preview",
		"sync": "dotenv-run -f .env -f .secrets -v -- svelte-kit sync",
		"test": "pnpm run test:integration && pnpm run test:unit",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@fontsource-variable/inter": "5.0.16",
		"@internationalized/date": "3.5.2",
		"@spectacular/eslint-config": "workspace:*",
		"@spectacular/prettier-config": "workspace:*",
		"@spectacular/typescript-config": "workspace:*",
		"@spectacular/ui": "workspace:*",
		"@spectacular/utils": "workspace:*",
		"@sveltejs/adapter-auto": "3.1.1",
		"@sveltejs/enhanced-img": "0.1.8",
		"@sveltejs/kit": "2.5.1",
		"@sveltejs/vite-plugin-svelte": "3.0.2",
		"@tailwindcss/container-queries": "0.1.1",
		"@tailwindcss/forms": "0.5.7",
		"@tailwindcss/typography": "0.5.10",
		"@types/eslint": "8.56.3",
		"@types/node": "20.11.20",
		"@typescript-eslint/eslint-plugin": "7.0.2",
		"@typescript-eslint/parser": "7.0.2",
		"@vincjo/datatables": "1.14.5",
		"autoprefixer": "10.4.17",
		"bits-ui": "0.18.2",
		"clsx": "2.1.0",
		"cmdk-sv": "0.0.13",
		"cssnano": "6.0.5",
		"eslint": "8.57.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.36.0-next.6",
		"formsnap": "0.5.1",
		"lucide-svelte": "0.338.0",
		"postcss": "8.4.35",
		"postcss-import": "16.0.1",
		"postcss-load-config": "5.0.3",
		"postcss-nested": "6.0.1",
		"prettier": "3.2.5",
		"prettier-plugin-svelte": "3.2.1",
		"prettier-plugin-tailwindcss": "0.5.11",
		"svelte": "4.2.12",
		"svelte-check": "3.6.4",
		"sveltekit-rate-limiter": "0.4.3",
		"sveltekit-superforms": "2.6.1",
		"sveltekit-view-transition": "0.5.3",
		"tailwind-merge": "2.2.1",
		"tailwind-variants": "0.2.0",
		"tailwindcss": "3.4.1",
		"tailwindcss-animate": "1.0.7",
		"tslib": "2.6.2",
		"typescript": "5.3.3",
		"vite": "5.1.4",
		"vite-plugin-tailwind-purgecss": "0.2.0",
		"vite-tsconfig-paths": "4.3.1",
		"vitest": "1.3.1",
		"zod": "3.22.4"
	},
	"optionalDependencies": {
		"@playwright/test": "1.41.2",
		"@vitest/ui": "1.3.1"
	}
