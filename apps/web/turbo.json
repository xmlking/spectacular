{
	"$schema": "https://turbo.build/schema.json",
	"extends": ["//"],
	"pipeline": {
		"build": {
			"dependsOn": ["generate"],
			"outputs": [".svelte-kit/**", ".vercel/**", "$houdini/**", "schema.graphql"],
			"env": [
				"VERCEL",
				"VERCEL_URL",
				"VERCEL_ENV",
				"BUILD_VERSION",
				"BUILD_TIME",
				"BUN_ENV",
				"SW_DEV",
				"npm_package_version"
			]
		},
		"generate": {
			"outputs": [".svelte-kit/**", "schema.graphql"]
		},
		"lint": {
			"dependsOn": ["generate"],
			"outputs": []
		},
		"preview": {},
		"special-task": {}
	}
}
