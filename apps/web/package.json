{
	"name": "web",
	"version": "0.4.0",
	"private": true,
	"scripts": {
		"dev": "vite dev --port 3000 --open",
		"dev:debug": "DEBUG=* vite dev",
		"dev:sw": "SW_DEV=true vite dev",
		"build": "vite build",
		"build:lib": "svelte-kit sync && svelte-package",
		"prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
		"preview": "NEXTAUTH_URL=http://localhost:4173 vite preview",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest run --coverage",
		"test:e2e": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"preinstall": "npx only-allow pnpm"
	},
	"devDependencies": {
		"@auth/core": "0.12.0",
		"@auth/sveltekit": "0.3.6",
		"@ethercorps/sveltekit-og": "2.0.1",
		"@neoconfetti/svelte": "1.0.0",
		"@neodrag/svelte": "2.0.3",
		"@popperjs/core": "2.11.8",
		"@skeletonlabs/skeleton": "1.10.0",
		"@svelte-put/resize": "3.0.0",
		"@sveltejs/adapter-auto": "2.1.0",
		"@sveltejs/adapter-node": "1.3.1",
		"@sveltejs/kit": "1.22.3",
		"@sveltejs/package": "2.2.0",
		"@tailwindcss/aspect-ratio": "0.4.2",
		"@tailwindcss/container-queries": "0.1.1",
		"@tailwindcss/forms": "0.5.4",
		"@tailwindcss/typography": "0.5.9",
		"@testing-library/jest-dom": "5.17.0",
		"@testing-library/svelte": "4.0.3",
		"@types/gtag.js": "0.0.12",
		"@types/js-cookie": "3.0.3",
		"@types/testing-library__jest-dom": "5.14.8",
		"@typescript-eslint/eslint-plugin": "6.1.0",
		"@typescript-eslint/parser": "6.1.0",
		"@vite-pwa/sveltekit": "0.2.5",
		"@vitest/coverage-c8": "0.33.0",
		"autoprefixer": "10.4.14",
		"c8": "8.0.0",
		"clsx": "2.0.0",
		"cross-fetch": "4.0.0",
		"cssnano": "6.0.1",
		"d3-force": "3.0.0",
		"d3-scale": "4.0.2",
		"daisyui": "3.3.1",
		"date-fns": "2.30.0",
		"eslint": "8.45.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-svelte": "2.32.4",
		"flowbite": "1.7.0",
		"flowbite-svelte": "0.40.1",
		"flowbite-svelte-icons": "0.2.6",
		"graphql": "16.7.1",
		"graphql-ws": "5.14.0",
		"houdini": "1.2.7",
		"houdini-svelte": "1.2.7",
		"jose": "4.14.4",
		"js-cookie": "3.0.5",
		"jsdom": "22.1.0",
		"layercake": "7.6.1",
		"msw": "1.2.3",
		"postcss": "8.4.27",
		"postcss-preset-env": "9.0.0",
		"prettier": "3.0.0",
		"prettier-plugin-svelte": "3.0.0",
		"prettier-plugin-tailwindcss": "0.4.1",
		"svelte": "4.1.1",
		"svelte-check": "3.4.6",
		"svelte-headless-table": "0.17.4",
		"svelte-heros-v2": "0.7.4",
		"svelte-select": "5.6.1",
		"svelte-time-distance": "0.0.3",
		"sveltekit-flash-message": "2.1.0",
		"sveltekit-superforms": "1.4.0",
		"tailwind-merge": "1.14.0",
		"tailwindcss": "3.3.3",
		"tslib": "2.6.0",
		"typescript": "5.1.6",
		"vite": "4.4.6",
		"vite-plugin-pwa": "0.16.4",
		"vitest": "0.33.0",
		"workbox-window": "7.0.0",
		"zod": "3.21.4",
		"eslint-config-custom": "workspace:*",
		"helpers": "workspace:*",
		"tailwind-config": "workspace:*",
		"ui": "workspace:*"
	},
	"optionalDependencies": {
		"@playwright/test": "1.36.1",
		"@vitest/ui": "0.33.0"
	},
	"type": "module",
	"dependencies": {
		"cookie": "0.5.0"
