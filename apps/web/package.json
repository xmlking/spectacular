{
	"name": "web",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"astro": "astro",
		"build": "astro check && astro build",
		"dev": "astro dev",
		"format": "concurrently pnpm:format:*",
		"format:js": "eslint --fix .",
		"format:prettier": "prettier --write .",
		"lint": "concurrently pnpm:lint:*",
		"lint:js": "eslint .",
		"lint:markup": "markuplint --config ../../.markuplintrc.cjs 'src/**/*.astro'",
		"lint:prettier": "prettier --check --plugin=prettier-plugin-astro --plugin=prettier-plugin-svelte .",
		"preview": "astro preview",
		"start": "astro dev",
		"test": "vitest"
	},
	"dependencies": {
		"sharp": "0.33.3"
	},
	"devDependencies": {
		"@astrojs/check": "0.5.10",
		"@astrojs/mdx": "2.3.1",
		"@astrojs/node": "8.2.5",
		"@astrojs/partytown": "2.1.0",
		"@astrojs/sitemap": "3.1.4",
		"@astrojs/starlight": "0.22.1",
		"@astrojs/svelte": "5.4.0",
		"@astrojs/tailwind": "5.1.0",
		"@astrojs/vercel": "7.5.4",
		"@fontsource-variable/inter": "5.0.18",
		"@fontsource/source-code-pro": "5.0.18",
		"@spectacular/eslint-config": "workspace:*",
		"@spectacular/prettier-config": "workspace:*",
		"@spectacular/typescript-config": "workspace:*",
		"@spectacular/utils": "workspace:*",
		"@tailwindcss/typography": "0.5.13",
		"astro": "4.7.1",
		"astro-expressive-code": "0.35.3",
		"astro-seo": "0.8.3",
		"lucide-svelte": "0.378.0",
		"mode-watcher": "0.3.0",
		"svelte": "4.2.15",
		"tailwind-merge": "2.3.0",
		"tailwindcss": "3.4.3",
		"typescript": "5.4.5",
		"vitest": "1.6.0"
	}
}
