		"@types/js-cookie": "3.0.3",
		"@typescript-eslint/eslint-plugin": "6.4.1",
		"@typescript-eslint/parser": "6.4.1",
		"@vite-pwa/sveltekit": "0.2.5",
		"@vitest/coverage-v8": "0.34.3",
