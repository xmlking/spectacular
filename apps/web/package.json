{
  "name": "web",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "astro check && astro build",
    "check": "biome check .",
    "check:fix": "biome check . --write",
    "clean": "git clean -xdf",
    "dev": "astro dev",
    "format": "biome format --write",
    "lint": "biome lint",
    "lint:markup": "markuplint --config ../../.markuplintrc.cjs 'src/**/*.astro'",
    "preview": "astro preview",
    "start": "astro dev",
    "test": "vitest",
    "test:unit:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@astrojs/mdx": "4.2.1",
    "@astrojs/node": "9.1.3",
    "@astrojs/partytown": "2.1.4",
    "@astrojs/starlight": "0.32.4",
    "@astrojs/svelte": "7.0.7",
    "@astrojs/vercel": "8.1.3",
    "@fontsource-variable/inter": "5.2.5",
    "@fontsource/source-code-pro": "5.2.5",
    "@tailwindcss/vite": "4.0.15",
    "@vitest/coverage-v8": "3.0.9",
    "astro": "5.5.4",
    "astro-expressive-code": "0.40.2",
    "astro-seo": "0.8.4",
    "lucide-svelte": "0.483.0",
    "mode-watcher": "0.5.1",
    "sharp": "0.33.5",
    "svelte": "5.25.2",
    "tailwind-merge": "3.0.2",
    "tailwindcss": "4.0.15",
    "typescript": "5.8.2",
    "vitest": "3.0.9"
  },
  "devDependencies": {
    "@spectacular/biome-config": "workspace:*",
    "@spectacular/typescript-config": "workspace:*"
  }
}
