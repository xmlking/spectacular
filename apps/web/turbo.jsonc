{
  "$schema": "https://turborepo.com/schema.json",
  "extends": ["//"],
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env",
        ".secrets"
      ],
      "outputs": [
        "build/**",
        "dist/**",
        ".vercel/output/**"
      ]
    },
    "astro": {},
    "preview": {
      "persistent": true
    }
  }
}
