{
	"name": "console2",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "dotenv-run vite dev",
		"dev:debug": "DEBUG=* vite dev",
		"dev:sw": "SW_DEV=true vite dev",
		"build": "dotenv-run vite build",
		"preview": "dotenv-run vite preview",
		"test": "pnpm run test:integration && pnpm run test:unit",
		"test:unit": "vitest",
		"test:unit:ui": "vitest --ui",
		"test:unit:coverage": "vitest run --coverage",
		"test:integration": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint:js": "eslint .",
		"lint:prettier": "prettier --check --ignore-path=../../.prettierignore .",
		"lint": "concurrently pnpm:lint:*",
		"format:js": "eslint --fix .",
		"format:prettier": "prettier --write --ignore-path=../../.prettierignore .",
		"format": "concurrently pnpm:format:*"
	},
	"devDependencies": {
		"@floating-ui/dom": "1.5.3",
		"@fontsource-variable/inter": "5.0.16",
		"@nhost/nhost-js": "2.2.18",
		"@skeletonlabs/skeleton": "2.6.0",
		"@skeletonlabs/tw-plugin": "0.3.0",
		"@sveltejs/adapter-auto": "2.1.1",
		"@sveltejs/adapter-node": "1.3.1",
		"@sveltejs/adapter-vercel": "3.1.0",
		"@sveltejs/enhanced-img": "0.1.5",
		"@sveltejs/kit": "1.29.1",
		"@tailwindcss/container-queries": "0.1.1",
		"@tailwindcss/forms": "0.5.7",
		"@tailwindcss/typography": "0.5.10",
		"@types/node": "20.10.4",
		"@typescript-eslint/eslint-plugin": "6.14.0",
		"@typescript-eslint/parser": "6.14.0",
		"@vitest/coverage-v8": "1.0.4",
		"autoprefixer": "10.4.16",
		"eslint": "8.55.0",
		"eslint-config-custom": "workspace:*",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.35.1",
		"formsnap": "0.4.1",
		"helpers": "workspace:*",
		"postcss": "8.4.32",
		"prettier": "3.1.1",
		"prettier-plugin-svelte": "3.1.2",
		"prettier-plugin-tailwindcss": "0.5.9",
		"svelte": "4.2.8",
		"svelte-adapter-bun": "0.5.1",
		"svelte-awesome-icons": "0.6.6",
		"svelte-check": "3.6.2",
		"svelte-meta-tags": "3.1.0",
		"sveltekit-rate-limiter": "0.4.1",
		"sveltekit-superforms": "1.11.0",
		"sveltekit-view-transition": "0.5.2",
		"tailwind-config": "workspace:*",
		"tailwind-merge": "2.1.0",
		"tailwindcss": "3.3.6",
		"tailwindcss-animate": "1.0.7",
		"tslib": "2.6.2",
		"typescript": "5.3.3",
		"ui": "workspace:*",
		"vite": "4.5.1",
		"vite-plugin-tailwind-purgecss": "0.1.4",
		"vitest": "0.34.6",
		"zod": "3.22.4"
	},
	"trustedDependencies": [
		"sharp"
	],
	"optionalDependencies": {
		"@playwright/test": "1.40.1",
		"@vitest/ui": "0.34.6"
	}
}
