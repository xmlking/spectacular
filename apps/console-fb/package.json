{
  "name": "console-fb",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "dotenv-run -f .env -f .secrets -v -- vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "dotenv-run -f .env -f .secrets -v -- vite dev",
    "dev:debug": "DEBUG=* dotenv-run -f .env -f .secrets -v '.*' -u -- vite dev",
    "dev:prod": "NODE_ENV=prod dotenv-run -f .env -f .secrets -v -- vite dev",
    "dev:sw": "SW_DEV=true dotenv-run -f .env -f .secrets -v -- vite dev",
    "format": "biome check --write",
    "generate": "concurrently pnpm:generate:*",
    "generate:pull-schema": "dotenv-run -f .env -f .secrets -v -- houdini pull-schema",
    "generate:svelte": "svelte-kit sync",
    "lint": "biome check",
    "preview": "ORIGIN=https://console.traefik.me:4173 NEXTAUTH_URL=http://localhost:4173 dotenv-run -f .env -f .secrets -v -- vite preview",
    "sync": "dotenv-run -f .env -f .secrets -v -- svelte-kit sync",
    "test": "pnpm run test:integration && pnpm run test:unit",
    "test:integration": "dotenv-run -f .env -f .secrets -v -- playwright test",
    "test:unit": "dotenv-run -f .env -f .secrets -v -- vitest",
    "test:unit:coverage": "dotenv-run -f .env -f .secrets -v -- vitest run --coverage || true",
    "test:unit:ui": "dotenv-run -f .env -f .secrets -v -- vitest --ui"
  },
  "dependencies": {
    "cookie": "0.6.0"
  },
  "devDependencies": {
    "@auth/core": "0.32.0",
    "@auth/sveltekit": "1.2.0",
    "@ethercorps/sveltekit-og": "3.0.0",
    "@neoconfetti/svelte": "2.2.1",
    "@neodrag/svelte": "2.0.6",
    "@popperjs/core": "2.11.8",
    "@skeletonlabs/skeleton": "2.10.0",
    "@spectacular/biome-config": "workspace:*",
    "@spectacular/typescript-config": "workspace:*",
    "@spectacular/utils": "workspace:*",
    "@svelte-put/resize": "3.0.1",
    "@sveltejs/adapter-auto": "3.2.1",
    "@sveltejs/adapter-node": "5.0.1",
    "@sveltejs/adapter-vercel": "5.3.1",
    "@sveltejs/enhanced-img": "0.2.1",
    "@sveltejs/kit": "2.5.16",
    "@sveltejs/vite-plugin-svelte": "3.1.1",
    "@tailwindcss/container-queries": "0.1.1",
    "@testing-library/jest-dom": "6.4.6",
    "@testing-library/svelte": "5.1.0",
    "@types/gtag.js": "0.0.20",
    "@types/js-cookie": "3.0.6",
    "@types/node": "20.14.2",
    "@vercel/analytics": "1.3.1",
    "@vercel/speed-insights": "1.0.11",
    "@vite-pwa/assets-generator": "0.2.4",
    "@vite-pwa/sveltekit": "0.5.0",
    "@vitest/coverage-v8": "1.6.0",
    "autoprefixer": "10.4.19",
    "c8": "10.1.1",
    "cross-fetch": "4.0.0",
    "cssnano": "7.0.2",
    "d3-force": "3.0.0",
    "d3-scale": "4.0.2",
    "date-fns": "3.6.0",
    "flowbite": "2.3.0",
    "flowbite-svelte": "0.46.1",
    "flowbite-svelte-blocks": "1.1.1",
    "flowbite-svelte-icons": "1.6.1",
    "flowbite-typography": "1.0.3",
    "graphql": "16.8.1",
    "graphql-ws": "5.16.0",
    "houdini": "1.2.47",
    "houdini-svelte": "1.2.47",
    "jose": "5.4.0",
    "js-cookie": "3.0.5",
    "jsdom": "24.1.0",
    "layercake": "8.3.0",
    "msw": "2.3.1",
    "postcss": "8.4.38",
    "postcss-preset-env": "9.5.14",
    "svelte": "4.2.18",
    "svelte-adapter-bun": "0.5.2",
    "svelte-check": "3.8.0",
    "svelte-headless-table": "0.18.2",
    "svelte-meta-tags": "3.1.2",
    "svelte-select": "5.8.3",
    "svelte-time-distance": "0.0.3",
    "sveltekit-flash-message": "2.4.4",
    "sveltekit-rate-limiter": "0.5.1",
    "sveltekit-superforms": "2.15.1",
    "sveltekit-view-transition": "0.5.3",
    "tailwind-merge": "2.3.0",
    "tailwind-variants": "0.2.1",
    "tailwindcss": "3.4.4",
    "tailwindcss-animate": "1.0.7",
    "throttle-debounce": "5.0.0",
    "tslib": "2.6.3",
    "typescript": "5.4.5",
    "vite": "5.2.13",
    "vite-plugin-pwa": "0.20.0",
    "vitest": "1.6.0",
    "zod": "3.23.8"
  },
  "optionalDependencies": {
    "@playwright/test": "1.44.1",
    "@vitest/ui": "1.6.0"
  }
}
