		"date-fns": "3.3.1",
		"eslint": "8.57.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.36.0-next.6",
		"flowbite": "2.3.0",
		"flowbite-svelte": "0.44.24",
		"flowbite-svelte-blocks": "1.1.0",
		"flowbite-svelte-icons": "1.4.1",
		"flowbite-typography": "1.0.3",
		"graphql": "16.8.1",
