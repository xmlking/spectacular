		"tailwind-merge": "2.2.1",
		"tailwind-variants": "0.2.0",
		"tailwindcss": "3.4.1",
		"tailwindcss-animate": "1.0.7",
		"tslib": "2.6.2",
		"typescript": "5.4.2",
