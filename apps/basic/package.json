{
	"name": "basic",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "vite dev --port 3001 --open",
		"dev:debug": "DEBUG=* vite dev",
		"dev:sw": "SW_DEV=true vite dev",
		"build": "vite build",
		"package": "vite package",
		"preview": "NEXTAUTH_URL=http://localhost:4173 vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check --ignore-path=../../.prettierignore . && eslint .",
		"format": "prettier --plugin-search-dir . --write --ignore-path=../../.prettierignore .",
		"test:unit": "vitest",
		"test:unit:ui": "vitest --ui",
		"test:unit:coverage": "vitest run --coverage",
		"test:integration": "playwright test",
		"test": "npm run test:integration && npm run test:unit",
		"preinstall": "npx only-allow pnpm"
	},
	"dependencies": {
		"highlight.js": "11.8.0",
		"@floating-ui/dom": "1.5.1"
	},
	"devDependencies": {
		"@playwright/test": "1.37.1",
		"@skeletonlabs/skeleton": "1.12.0",
		"@skeletonlabs/tw-plugin": "0.0.0",
		"@sveltejs/adapter-auto": "2.1.0",
		"@sveltejs/kit": "1.22.6",
		"@tailwindcss/forms": "0.5.4",
		"@tailwindcss/typography": "0.5.9",
		"@types/node": "20.5.1",
		"@typescript-eslint/eslint-plugin": "6.4.0",
		"@typescript-eslint/parser": "6.4.0",
		"autoprefixer": "10.4.15",
		"eslint": "8.47.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-svelte": "2.32.4",
		"postcss": "8.4.28",
		"prettier": "3.0.2",
		"prettier-plugin-svelte": "3.0.3",
		"svelte": "4.2.0",
		"svelte-check": "3.5.0",
		"tailwindcss": "3.3.3",
		"tslib": "2.6.2",
		"typescript": "5.1.6",
		"vite": "4.4.9",
		"vite-plugin-tailwind-purgecss": "0.1.3",
		"vitest": "0.34.2",
		"eslint-config-custom": "workspace:*",
		"helpers": "workspace:*",
		"tailwind-config": "workspace:*",
		"ui": "workspace:*"
	},
	"optionalDependencies": {
		"@playwright/test": "1.37.1",
		"@vitest/ui": "0.33.0"
	},
	"type": "module"
}
