{
	"name": "tailwind-config",
	"version": "0.0.0",
	"description": "tailwind config for use in svelte websites",
	"main": "tailwind.config.ts",
	"types": "tailwind.config.ts",
	"exports": {
		"./tailwind.config.ts": "./tailwind.config.ts",
		"./postcss.config.cjs": "./postcss.config.cjs"
	},
	"license": "MIT",
	"scripts": {
		"lint": "prettier --check --ignore-path=../../.prettierignore . && eslint \".\"",
		"format": "prettier --write --ignore-path=../../.prettierignore ."
	},
	"devDependencies": {
		"autoprefixer": "10.4.15",
		"postcss": "8.4.29",
		"tailwindcss": "3.3.3"
	},
	"peerDependencies": {
		"@tailwindcss/aspect-ratio": "0.4.2",
		"@tailwindcss/container-queries": "0.1.1",
		"@tailwindcss/forms": "0.5.5",
		"@tailwindcss/line-clamp": "0.4.4",
		"@tailwindcss/typography": "0.5.9",
		"eslint-config-custom": "workspace:*",
		"flowbite-svelte": "0.44.4",
		"flowbite-svelte-blocks": "0.4.14",
		"flowbite-svelte-icons": "0.3.6",
		"flowbite-typography": "1.0.3",
		"tailwind-merge": "1.14.0",
		"tailwindcss-animate": "1.0.6"
	}
}
