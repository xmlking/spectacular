{
	"name": "ui",
	"version": "0.0.0",
	"description": "Styles and components for use in svelte websites",
	"license": "MIT",
	"scripts": {
		"package": "svelte-kit package && tsx --esm ./scripts/cleanupPackage.ts && rm ./dist/package.json",
		"lint": "prettier --check --ignore-path=../../.prettierignore . && eslint \".\"",
		"format": "prettier --write --ignore-path=../../.prettierignore ."
	},
	"devDependencies": {
		"eslint-config-custom": "workspace:*",
		"svelte": "4.2.0",
		"tailwind-config": "workspace:*",
		"tsconfig": "workspace:*",
		"tailwindcss": "3.3.3"
	},
	"type": "module",
	"exports": {
		"./package.json": "./package.json",
		".": "./src/index.ts"
	},
	"types": "src/index.ts",
	"svelte": "src/index.ts",
	"module": "src/index.ts",
	"publishConfig": {
		"svelte": "./dist/index.js",
		"files": [
			"dist"
		],
		"exports": {
			"./package.json": "./package.json",
			".": "./dist/index.js"
		},
		"types": "./dist/index.d.ts"
	}
}
