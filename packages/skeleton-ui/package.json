{
  "name": "@spectacular/skeleton",
  "description": "Shared skeleton UI components",
  "sideEffects": false,
  "type": "module",
  "imports": {
    "#utils": "./src/utils/index.js",
    "#*": "./src/*"
  },
  "exports": {
    ".": {
      "types": "./src/components/index.ts",
      "svelte": "./src/components/index.ts",
      "default": "./src/components/index.ts"
    },
    "./actions": "./src/actions/index.ts",
    "./actions/*": "./src/actions/*.ts",
    "./components": "./src/components/index.ts",
    "./components/*": "./src/components/*/index.ts",
    "./utils": "./src/utils/index.ts",
    "./styles/default": "./styles/default.pcss",
    "./postcss.config": "./postcss.config.cjs",
    "./tailwind.config": "./tailwind.config.ts"
  },
  "scripts": {
    "format": "biome check --write",
    "lint": "biome check",
    "test": "vitest run",
    "test:unit:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@floating-ui/dom": "1.6.7",
    "@fontsource-variable/inter": "5.0.18",
    "@skeletonlabs/skeleton": "2.10.1",
    "@skeletonlabs/tw-plugin": "0.4.0",
    "@spectacular/biome-config": "workspace:*",
    "@spectacular/typescript-config": "workspace:*",
    "@spectacular/utils": "workspace:*",
    "@tailwindcss/container-queries": "0.1.1",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/line-clamp": "0.4.4",
    "@tailwindcss/typography": "0.5.13",
    "@tsconfig/svelte": "5.0.4",
    "@types/node": "20.14.10",
    "@vincjo/datatables": "1.14.10",
    "@vitest/coverage-v8": "1.6.0",
    "autoprefixer": "10.4.19",
    "bits-ui": "0.21.11",
    "clsx": "2.1.1",
    "cssnano": "7.0.3",
    "formsnap": "1.0.1",
    "lucide-svelte": "0.399.0",
    "postcss": "8.4.39",
    "postcss-import": "16.1.0",
    "postcss-load-config": "6.0.1",
    "sveltekit-superforms": "2.15.2",
    "tailwind-merge": "2.3.0",
    "tailwind-variants": "0.2.1",
    "tailwindcss": "3.4.4",
    "tslib": "2.6.3",
    "typescript": "5.5.3",
    "vite": "5.2.13",
    "vitest": "1.6.0",
    "zod": "3.23.8"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.0.0",
    "svelte": "^4.0.0"
  }
}
