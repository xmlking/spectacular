{
	"name": "@spectacular/eslint-config",
	"description": "eslint config files",
	"exports": {
		"./all": "./all.js",
		"./node": "./node.js",
		"./svelte": "./svelte.js"
	},
	"main": "all.js",
	"scripts": {
		"format": "prettier --write . && eslint --fix .",
		"lint": "prettier --check . && eslint ."
	},
	"devDependencies": {
		"@spectacular/prettier-config": "workspace:*",
		"@spectacular/typescript-config": "workspace:*"
	},
	"peerDependencies": {
		"@typescript-eslint/eslint-plugin": "6.15.0",
		"@typescript-eslint/parser": "6.15.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-config-turbo": "1.11.2",
		"eslint-import-resolver-typescript": "3.6.1",
		"eslint-plugin-astro": "0.31.0",
		"eslint-plugin-import": "2.29.1",
		"eslint-plugin-mdx": "3.0.1",
		"eslint-plugin-svelte": "2.36.0-next.5",
		"eslint-plugin-unused-imports": "3.0.0"
	}
}
