{
	"$schema": "https://turbo.build/schema.json",
	"globalDotEnv": [".env.local", ".env"],
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".svelte-kit/**", ".vercel/**"]
		},
		"preview": {},
		"lint": {},
		"format": {},
		"test": {
			"dotEnv": [".env.test", ".env"]
		},
		"check": {},
		"test:unit": {},
		"test:unit:coverage": {},
		"test:integration": {},
		"test:watch": {
			"cache": false
		},
		"clean": {
			"cache": false
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
