{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".svelte-kit/**", ".vercel/**"],
			"env": [
				"VERCEL_URL",
				"VERCEL_ENV",
				"NODE_ENV",
				"DATABASE_URL",
				"GITHUB_ID",
				"GITHUB_SECRET",
				"GITHUB_ADMIN_ID",
				"GITHUB_ADMIN_SECRET",
				"UPLOADTHING_SECRET",
				"UPLOADTHING_APP_ID",
				"EDGE_CONFIG",
				"NEXTAUTH_SECRET",
				"NEXTAUTH_URL",
				"RESEND_API_KEY",
				"PORT",
				"USERNAME",
				"PASSWORD"
			]
		},
		"lint": {},
		"format": {},
		"test": {},
		"test:unit": {},
		"test:integration": {},
		"test:watch": {
			"cache": false
		},
		"clean": {
			"cache": false
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
