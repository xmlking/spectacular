      "@testing-library/dom": "10.4.1",
      "@testing-library/jest-dom": "6.6.4",
      "@testing-library/svelte": "5.2.8",
      "@turbo/gen": "2.5.8",
      "@types/bun": "1.2.16",
      "@types/dom-chromium-ai": "0.0.10",
      "@types/node": "24.1.0",
      "@vitest/browser": "3.2.4",
      "@vitest/coverage-v8": "3.2.4",
      "@vitest/ui": "3.2.4",
      "ai": "5.0.0",
      "astro": "5.12.8",
      "bits-ui": "2.11.4",
      "cspell": "9.2.1",
      "flags": "4.0.1",
      "formsnap": "2.0.1",
      "houdini": "1.5.10",
      "houdini-svelte": "2.1.20",
      "jsdom": "26.1.0",
      "playwright": "1.56.0",
      "runed": "0.34.0",
      "svelte": "5.39.9",
      "svelte-check": "4.3.2",
      "svelte-meta-tags": "4.3.0",
      "svelte-sonner": "1.0.5",
