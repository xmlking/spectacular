    },
    "catalogs": {
      "svelte4": {
        "@ai-sdk/svelte": "1.1.24",
        "@inlang/paraglide-sveltekit": "0.14.0",
        "@skeletonlabs/skeleton": "2.11.0",
        "@sveltejs/enhanced-img": "0.3.10",
        "@sveltejs/kit": "2.21.0",
        "@sveltejs/vite-plugin-svelte": "3.1.2",
        "@xyflow/svelte": "0.1.39",
