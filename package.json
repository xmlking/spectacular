      "@aibrow/dom-types": "2.0.5",
      "@biomejs/biome": "2.2.5",
      "@changesets/changelog-github": "0.5.1",
      "@changesets/cli": "2.29.7",
      "@dotenv-run/cli": "1.3.6",
      "@inlang/paraglide-js": "2.4.0",
