{
  "version": "0.2",
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/master/cspell.schema.json",
  "enabledLanguageIds": [
    "asciidoc",
    "css",
    "go",
    "html",
    "javascript",
    "json",
    "markdown",
    "plaintext",
    "python",
    "rust",
    "scss",
    "text",
    "typescript",
    "typescriptreact",
    "yml",
    "proto3",
    "svelte"
  ],
  "dictionaries": ["html", "css", "typescript", "node", "softwareTerms", "companies"],
  "words": [
    "Aceternity",
    "astro",
    "astrojs",
    "buildx",
    "bunx",
    "chinthagunta",
    "clsx",
    "cmdk",
    "codeql",
    "combobox",
    "daisyui",
    "degit",
    "devdeps",
    "Felte",
    "flowbite",
    "FOUC",
    "grpc",
    "hasrua",
    "hasura",
    "inlang",
    "Kometa",
    "Konsta",
    "Kustomize",
    "legos",
    "log",
    "lucide",
    "mdsvex",
    "mkdir",
    "navi",
    "nhost",
    "nixos",
    "OIDC",
    "pcss",
    "pgcrypto",
    "postgresdb",
    "proto",
    "protobuf",
    "psql",
    "robotstxt",
    "shadcn",
    "substrs",
    "sumanth",
    "sveltekit",
    "sveltin",
    "svitejs",
    "swyxkit",
    "tailwindcss",
    "TAURI",
    "Threlte",
    "timeout",
    "timestamptz",
    "tini",
    "totp",
    "vitebook",
    "zodfd"
  ],
  "language": "en,en-GB,en-US",
  // This makes the spell checker more lenient when two words are jammed together.
  "allowCompoundWords": true,
  "useGitignore": true,
  "files": ["**/*.md"],
  "ignoreRegExpList": ["Email", "\\_\\w+", "\\#\\w+"],
  "ignorePaths": [
    ".git",
    "**/build/**",
    "**/dist/**",
    "**/static/**",
    "**/node_modules/**",
    "package.json",
    "pnpm-lock.yaml",
    "*.svg",
    ".vscode/**",
    "/nhost/",
    "coverage/",
    "TODO/",
    "/docs/images/**",
    "/apps/*/storybook-static/",
    "/apps/*/$houdini/",
    "/apps/*/schema.graphql",
    "CHANGELOG.md",
    "compose.yml",
    "flake.nix"
  ]
}
