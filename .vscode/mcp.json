{
  "servers": {
    "context7": {
      "type": "http",
      "url": "https://mcp.context7.com/mcp"
    },
    "svelte": {
      "url": "https://svelte-llm.khromov.se/mcp/mcp"
    },
    "Playwright": {
      "command": "npx",
      "args": ["@playwright/mcp@latest"],
      "env": {}
    },
    "jsrepo": {
      "command": "npx",
      "args": ["jsrepo", "mcp"]
    },
    "mastra": {
      "command": "npx",
      "args": ["-y", "@mastra/mcp-docs-server@latest"],
      "type": "stdio"
    }
  }
}
